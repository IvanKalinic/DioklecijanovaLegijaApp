@model DioklecijanovaLegija.Models.Member
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>Details</h2>

<h2>@Model.Name</h2>
<ul>
    <li>Birhtdate: @Model.BirthDate.Value.ToShortDateString()</li>  @*value osigurava da tek ako nie null moze prominit u shortdatestring*@
    <li>Phone number: @Model.PhoneNumber</li>
    <li>Soldier Type: @Model.SoldierType.Type</li>
</ul>
